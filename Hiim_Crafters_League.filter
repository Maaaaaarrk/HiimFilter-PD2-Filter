ItemDisplay[UNI ELT]: %BORDER-68%%NAME%{%NAME%}
ItemDisplay[RARE ID]: %NAME%{%NAME%}
ItemDisplay[CRAFT ID]: %NAME%{%NAME%}
ItemDisplay[RW]: %MAP-68%%NAME%{%NAME%}
ItemDisplay[UNI]: %MAP-68%%NAME%{%NAME%}
ItemDisplay[SET]: %PX-84%%NAME%{%NAME%}
ItemDisplay[CRAFT]: %NAME%{%NAME%}
ItemDisplay[SHOP !NMAG]: %NAME%{%NAME%}
//-----------------------------------------------------------------------------------

// ------------------------------------------------------------------------------
// Leveling trash
// ------------------------------------------------------------------------------
ItemDisplay[!SHOP CHEST NORM !INF !SUP SOCK=0 NMAG CLVL<31]: %NAME%{%NAME%}
ItemDisplay[!SHOP (WEAPON AND !THROWING) NORM !INF !SUP SOCK=0 NMAG CLVL<31]: %NAME%{%NAME%}
ItemDisplay[!SHOP (HELM OR CIRC) NORM !INF !SUP SOCK=0 NMAG CLVL<31]: %NAME%{%NAME%}
ItemDisplay[!SHOP SHIELD NORM !INF !SUP SOCK=0 NMAG CLVL<31]: %NAME%{%NAME%}
ItemDisplay[!SHOP CHEST (EXC OR ELT) !INF !SUP SOCK=0 NMAG (CLVL<70 OR FILTLVL<3)]: %NAME%{%NAME%}
ItemDisplay[!SHOP (WEAPON AND !THROWING) (EXC OR ELT) !INF !SUP SOCK=0 NMAG (CLVL<70 OR FILTLVL<3)]: %NAME%{%NAME%}
ItemDisplay[!SHOP (HELM OR CIRC) (EXC OR ELT) !INF !SUP SOCK=0 NMAG (CLVL<70 OR FILTLVL<3)]: %NAME%{%NAME%}
ItemDisplay[!SHOP SHIELD (EXC OR ELT) !INF !SUP SOCK=0 NMAG (CLVL<70 OR FILTLVL<3)]: %NAME%{%NAME%}

// Alternate Bases when lower CLVL
ItemDisplay[!SHOP WEAPON ELT SUP (SOCK=0 OR SOCK>2) (CLVL<70 OR FILTLVL<3)]: %DOT-D6%%NAME%{%NAME%}
ItemDisplay[!SHOP WEAPON ELT !INF NMAG (SOCK>2) ILVL<90 (CLVL<70 OR FILTLVL<3)]: %DOT-D6%%NAME%{%NAME%}
ItemDisplay[!SHOP WEAPON ELT ETH SUP (SOCK=0 OR SOCK>2) (CLVL<70 OR FILTLVL<3)]: %MAP-D6%O  %NAME%  O{%NAME%}
ItemDisplay[!SHOP WEAPON ELT ETH !INF NMAG !THROWING (SOCK=0 OR SOCK>2) (CLVL<70 OR FILTLVL<3)]: %DOT-D6%%NAME%{%NAME%}
ItemDisplay[!SHOP (EQ1 OR EQ2 OR EQ3) ELT SUP (SOCK=0 OR SOCK>2) (CLVL<70 OR FILTLVL<3)]: %MAP-D6%%NAME%{%NAME%}
ItemDisplay[!SHOP (EQ1 OR EQ2 OR EQ3) ELT !INF NMAG (SOCK>2) ILVL<90 (CLVL<70 OR FILTLVL<3)]: %DOT-D6%%NAME%{%NAME%}
ItemDisplay[!SHOP (EQ1 OR EQ2 OR EQ3) ELT ETH !INF NMAG (SOCK=0 OR SOCK>2) (CLVL<70 OR FILTLVL<3)]: %DOT-D6%%NAME%{%NAME%}

// GLOBAL FILTERING - SHOW
ItemDisplay[!SHOP (ARMOR OR WEAPON) (MAG OR RARE OR SET OR UNI OR CRAFT) ID]: %NAME%{%NAME%}
ItemDisplay[!SHOP (rin OR amu) (MAG OR RARE OR SET OR UNI OR CRAFT) ID]: %NAME%{%NAME%}
//-----------------------------------------------------------------------------------

// ------------------------------------------------------------------------------
// Filter level Setting limiters
// ------------------------------------------------------------------------------

ItemDisplay[!SHOP FILTLVL>2 NMAG !RW WEAPON !(ETH OR WP9 OR CL5)]:
ItemDisplay[!SHOP FILTLVL>2 NMAG !RW ARMOR !(CHEST AND ETH)]:

//Hide 0 socket non elite filter lvl>1
ItemDisplay[!SHOP (ARMOR OR WEAPON) NMAG SOCK=0 !leg !ELT FILTLVL>1]:

//Hide non eth weapons unid mag in hell FILTLVL>1
ItemDisplay[!SHOP WEAPON MAG !ETH !ID ILVL<85 FILTLVL>1 DIFF=2]:

//Hide non eth weapons unid rare in hell clvl > 85
ItemDisplay[!SHOP WEAPON RARE !ETH !ID ILVL<85 FILTLVL>2 DIFF=2]:

// Hide all remaining magic <85 Ilvl
ItemDisplay[!SHOP MAG !ID ILVL<85 FILTLVL>1]:

// Hide all bases normal quality not called out above
ItemDisplay[!SHOP (ARMOR OR WEAPON) NMAG NORM !leg DIFF=2 FILTLVL>1]:

// Hide all bases Exceptional quality not called out above
ItemDisplay[!SHOP (ARMOR OR WEAPON) NMAG EXC !leg DIFF=2 FILTLVL>1]:

// Hide all bases not called out above
ItemDisplay[!SHOP (ARMOR OR WEAPON) NMAG SOCK<2 !leg DIFF=2 FILTLVL>2]:

// Hide all magic !eth on strict+
ItemDisplay[!SHOP MAG !ID !ETH FILTLVL>2]:

// Hide all magic on endgame
ItemDisplay[!SHOP MAG !ID FILTLVL>4]:

// Hide all items that arent called out in filter on endgame
ItemDisplay[!SHOP (ARMOR OR WEAPON) !leg DIFF=2 FILTLVL>6]: 
