// Start formatting
ItemDisplay[jew ID (STAT91<0 OR STAT501>0 OR RES>0 OR FHR>0 OR STAT86>0 OR STAT138>0 OR IAS>0 OR EDAM>0 OR MINDMG>0 OR MAXDMG>0 OR MANA>14 OR REPLIFE>2 OR STR+DEX>9)]: {%NAME%}%CONTINUE%

// - req STAT91
ItemDisplay[jew STAT91<0]: %SAGE%-Req %NAME%%CONTINUE%{%NAME%}

// IAS
ItemDisplay[jew IAS>0]: %GOLD%IAS %NAME%%CONTINUE%{%NAME%}

// Open Wounds
ItemDisplay[jew !UNI STAT501>0]: %RED%+%STAT501% OW %NAME%%CONTINUE%{%NAME%}

// ED
ItemDisplay[jew EDAM>0]: %GREEN%%EDAM%%ED %NAME%%CONTINUE%{%NAME%}

// Max Dmg
ItemDisplay[jew STAT24>0 !(STAT23>0)]: %RED%%STAT24% MaxDMG %NAME%%CONTINUE%{%NAME%}

// Min Dmg
ItemDisplay[jew STAT23>0 !(STAT24>0)]: %BLUE%%STAT23% MinDMG %NAME%%CONTINUE%{%NAME%}

// Dmg Range
