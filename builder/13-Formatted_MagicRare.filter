//-----------------------------------------------------------------------------------
// JEWELS - ITEM TAGS (Extends on some of the Shopping and tags)
//-----------------------------------------------------------------------------------
// Add re-roll tag for all rare jewels that do not hit a stat below
ItemDisplay[jew ID RARE ILVL>67 STAT486=0]: %NAME%{%TAN%Re-Roll%WHITE%: This %YELLOW%Jewel %TAN%+ %LIGHT_GRAY%3 Perfect Skulls%CL%}%CONTINUE%
ItemDisplay[jew ID MAG ALVL>65 STAT486=0]: %NAME%{%WHITE%1 in 863 chance to get Enhanced Damage & IAS%CL%%TAN%Re-Roll%WHITE%: This %BLUE%Jewel %TAN%+ %WHITE%Any 3 Perfect Gems%CL%%NAME%}%CONTINUE%

// Jewel rare re-roll level
ItemDisplay[RARE jew ID RARE]: %NAME%{%TAN%Re-Rolled ILvl: %WHITE%%REROLLALVL%%CL%%NAME%}%CONTINUE%

// Start formatting
ItemDisplay[jew ID (STAT91<0 OR STAT501>0 OR RES>0 OR FHR>0 OR STAT86>0 OR STAT138>0 OR IAS>0 OR EDAM>0 OR MINDMG>0 OR MAXDMG>0 OR MANA>14 OR REPLIFE>2 OR STR+DEX>9)]: {%NAME%}%CONTINUE%

// - req STAT91
ItemDisplay[jew STAT91<0]: %SAGE%-Req %NAME%%CONTINUE%{%NAME%}

// IAS
ItemDisplay[jew IAS>0]: %GOLD%IAS %NAME%%CONTINUE%{%NAME%}

// Open Wounds
ItemDisplay[jew !UNI STAT501>0]: %RED%+%STAT501% OW %NAME%%CONTINUE%{%NAME%}

// ED
ItemDisplay[jew EDAM>0]: %GREEN%%EDAM%%ED %NAME%%CONTINUE%{%NAME%}

// Max Dmg
ItemDisplay[jew STAT24>0 !(STAT23>0)]: %RED%%STAT24% MaxDMG %NAME%%CONTINUE%{%NAME%}

// Min Dmg
ItemDisplay[jew STAT23>0 !(STAT24>0)]: %BLUE%%STAT23% MinDMG %NAME%%CONTINUE%{%NAME%}

// Dmg Range
ItemDisplay[jew STAT23>0 STAT24>0]: %RED%+(%STAT23%-%STAT24%)DMG %NAME%%CONTINUE%{%NAME%}

// FHR
ItemDisplay[jew FHR>0]: %GOLD%FHR %NAME%%CONTINUE%{%NAME%}

// REPLIFE
ItemDisplay[jew (REPLIFE>2)]: %RED%%REPLIFE%rep %NAME%%CONTINUE%

// Mana
ItemDisplay[jew (MANA>14)]: %BLUE%%MANA%mana %NAME%%CONTINUE%

// STATS
ItemDisplay[jew (STR+DEX>9)]: Stat %NAME%%CONTINUE%
ItemDisplay[jew (STR+DEX>15)]: Fat%NAME%%CONTINUE%
ItemDisplay[jew (STR+DEX>9)]: %LIGHT_GRAY%%NAME%%CONTINUE%

// Life After Kill
ItemDisplay[jew !UNI !SET STAT86>0]: %RED%%STAT86% LAEK %NAME%%CONTINUE%{%NAME%}

// Mana After Kill
ItemDisplay[jew !UNI !SET STAT138>0]: %BLUE%%STAT138% MAEK %NAME%%CONTINUE%{%NAME%}

// All RES
ItemDisplay[jew !UNI !SET RES>0]: %PURPLE%%RES%% ALL Res %NAME%%CONTINUE%{%NAME%}


// Top line formatting
ItemDisplay[jew ID (STAT91<0 OR STAT501>0 OR RES>0 OR FHR>0 OR STAT86>0 OR STAT138>0 OR IAS>0 OR EDAM>0 OR MINDMG>0 OR MAXDMG>0 OR MANA>14 OR REPLIFE>2 OR STR+DEX>9)]: %DOT-50%%NAME%%CL%%CONTINUE%{%NAME%}

// BIG jewel callouts
ItemDisplay[jew FHR>0 STAT91<0 RES>0]: %NAME%%PURPLE%BIG %CONTINUE%
ItemDisplay[jew (FHR>0 OR STAT91<0) RES>14]: %NAME%%PURPLE%BIG %CONTINUE%
ItemDisplay[jew (FHR>0 OR STAT91<0) (FRES+CRES+LRES+PRES>25) RES=0]: %NAME%%PURPLE%BIG %CONTINUE%
