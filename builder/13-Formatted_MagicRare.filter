// FHR
ItemDisplay[jew FHR>0]: %GOLD%FHR %NAME%%CONTINUE%{%NAME%}

// REPLIFE
ItemDisplay[jew (REPLIFE>2)]: %RED%%REPLIFE%rep %NAME%%CONTINUE%

// Mana
ItemDisplay[jew (MANA>14)]: %BLUE%%MANA%mana %NAME%%CONTINUE%

// STATS
ItemDisplay[jew (STR+DEX>9)]: Stat %NAME%%CONTINUE%
ItemDisplay[jew (STR+DEX>15)]: Fat%NAME%%CONTINUE%
ItemDisplay[jew (STR+DEX>9)]: %LIGHT_GRAY%%NAME%%CONTINUE%

// Life After Kill
ItemDisplay[jew !UNI !SET STAT86>0]: %RED%%STAT86% LAEK %NAME%%CONTINUE%{%NAME%}

// Mana After Kill
